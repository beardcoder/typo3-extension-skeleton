{
    "name": "vendor/typo3-extension-skeleton",
    "type": "typo3-cms-extension",
    "description": "TYPO3 v13 Extension Skeleton with Auto-Documentation",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "^8.2",
        "typo3/cms-core": "^13.0",
        "typo3/cms-extbase": "^13.0",
        "typo3/cms-fluid": "^13.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Vendor\\ExtensionSkeleton\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "extension_skeleton"
        }
    },
    "scripts": {
        "docs:generate": "php Build/PhpDocGenerator.php",
        "docs:dev": "cd docs && npm run docs:dev",
        "docs:build": "cd docs && npm run docs:build",
        "phpstan": "phpstan analyse Classes",
        "cs-fix": "php-cs-fixer fix"
    }
}
