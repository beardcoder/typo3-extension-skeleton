<?xml version="1.0" encoding="UTF-8"?>
<phpdocumentor
    configVersion="3"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="https://www.phpdoc.org"
>
    <template name="./.Build/vendor/saggre/phpdocumentor-markdown/themes/markdown" />

    <paths>
        <output>docs</output>
        <cache>.phpdoc/cache</cache>
    </paths>

    <version number="latest">
        <api>
            <source dsn=".">
                <path>Classes</path>
            </source>
            <ignore>
                <path>.Build/**/*</path>
                <path>vendor/**/*</path>
            </ignore>
        </api>
    </version>

</phpdocumentor>